{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "16.x",
    "npm": ">=6.13.1"
  },
  "proxy": "http://localhost:3001",
  "dependencies": {
    "@appsignal/javascript": "^1.3.19",
    "@craco/craco": "^6.4.3",
    "@mars/heroku-js-runtime-env": "^3.0.2",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@reduxjs/toolkit": "^1.6.2",
    "antd": "^4.17.3",
    "antd-mask-input": "^0.1.15",
    "craco-less": "^1.20.0",
    "dayjs": "^1.10.7",
    "deepmerge": "^4.2.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "hash-anything": "^1.3.2",
    "i18next": "^21.6.0",
    "i18next-browser-languagedetector": "^6.1.2",
    "install": "^0.13.0",
    "jwt-decode": "^3.0.0",
    "papaparse": "^5.3.1",
    "postcss": "^8.4.5",
    "postcss-cli": "^9.0.2",
    "prettier": "^2.5.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.15.1",
    "react-redux": "^7.2.6",
    "react-router-dom": "^5.3.0",
    "react-scripts": "^4.0.3",
    "react-use-hotjar": "^1.2.0",
    "tailwindcss": "^3.0.5",
    "uuid": "^8.3.2",
    "xlsx": "^0.17.4"
  },
  "scripts": {
    "build": "craco build",
    "build:style": "tailwind build src/tailwind.css -o src/tailwind.generated.css",
    "format": "prettier --write \"**/*.+(js|jsx|json|yml|yaml|css|md|vue)\"",
    "lint": "yarn eslint ./src",
    "lint:fix": "yarn lint --fix",
    "prebuild": "yarn run build:style",
    "prestart": "yarn run build:style",
    "start": "craco start",
    "test": "craco test --env=jest-environment-jsdom-sixteen --coverage --watchAll",
    "test-once": "CI=true craco test"
  },
  "prettier": {
    "printWidth": 80,
    "tabWidth": 2,
    "singleQuote": true,
    "trailingComma": "none",
    "bracketSpacing": true,
    "semi": false,
    "useTabs": false,
    "arrowParens": "avoid"
  },
  "eslintConfig": {
    "plugins": [
      "react",
      "prettier"
    ],
    "extends": [
      "react-app",
      "eslint:recommended",
      "plugin:react/recommended",
      "prettier",
      "prettier/react"
    ],
    "rules": {
      "prettier/prettier": "error"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!jest/**",
      "!build/**",
      "!src/index.js",
      "!src/util/commands.js"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!geodesy).+\\.js$"
    ],
    "globalSetup": "./global-setup.js"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/dom": "^8.11.1",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "autoprefixer": "^10.4.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-syntax-decorators": "^6.13.0",
    "eslint-plugin-flowtype": "^5.10.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "faker": "^5.5.3",
    "jest-environment-jsdom-sixteen": "^2.0.0",
    "typescript": "^4.5.3"
  }
}
